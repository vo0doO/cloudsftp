{
  "name": "sftpclient",
  "productName": "sFTP Client",
  "description": "Transfer files with FTP, FTPIS, FTPES and SFTP (Including SSH Terminal)",
  "author": "sFTP Client Ltd",
  "version": "3.3.17",
  "main": "./app/app.js",
  "license": "Proprietary",
  "keywords": [
    "sftp",
    "ftp",
    "ftps",
    "ftpis",
    "ftpes",
    "ssh",
    "filezilla",
    "cyberduck",
    "winscp",
    "crossftp",
    "cuteftp",
    "transfer",
    "terminal",
    "tls",
    "nas",
    "server",
    "port forwarding",
    "dropbox",
    "amazon s3"
  ],
  "scripts": {
    "start": "electron .",
    "start-dev": "electron . --debug",
    "dev": "ng build --watch --base-href=.",
    "mas": "ng build --configuration mas",
    "mas-lite": "ng build --configuration mas-lite",
    "was": "ng build --configuration was",
    "was-lite": "ng build --configuration was-lite",
    "las": "ng build --configuration las",
    "prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "echo $npm_package_version",
    "extract-translations": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format namespaced-json --format-indentation '  ' --marker _",
    "translate": "php ./scripts/translate.php",
    "package": "npm-run-all package:*",
    "package-all": "electron-packager . \"sFTP Client\" --ignore=\"^angular.json|^Jenkinsfile|^.idea|^.git|^.editorconfig|^.gitignore|^karma.conf.js|^package-lock.json|^protractor.conf.js|^README.md|^tsconfig.json|^tslint.json|^e2e|^src|^scripts|^snap|^release\" --overwrite --asar --icon=\"app/assets/icons/png/1024x1024.png\" --prune=true --out=\"release/builds\"",
    "package-mas": "./scripts/mac/store/pro.sh",
    "package-mas-lite": "./scripts/mac/store/lite.sh",
    "package-mac": "./scripts/mac/installer.sh",
    "package-windows": "electron-packager . \"sFTP Client\" --ignore=\"^/angular.json|^/tsconfig-serve.json|^/Jenkinsfile|^/.idea|^/.git|^/.editorconfig|^/.gitignore|^/karma.conf.js|^/package-lock.json|^/protractor.conf.js|^/README.md|^/tsconfig.json|^/tslint.json|^/src|^/e2e|^/scripts|^/snap|^/release|^/build\" --overwrite --platform=win32 --asar  --arch=ia32 --icon=\"app/assets/icons/win/icon.ico\" --prune=true --out=\"release/builds\" --version-string.CompanyName=\"sFTP Client\" --version-string.FileDescription=\"sFTP Client\" --version-string.ProductName=\"sFTP Client\"",
    "package-windows64": "electron-packager . \"sFTP Client\" --ignore=\"^/angular.json|^/tsconfig-serve.json|^/Jenkinsfile|^/.idea|^/.git|^/.editorconfig|^/.gitignore|^/karma.conf.js|^/package-lock.json|^/protractor.conf.js|^/README.md|^/tsconfig.json|^/tslint.json|^/src|^/e2e|^/scripts|^/snap|^/release|^/build\" --overwrite --platform=win32 --asar --arch=ia32 --icon=\"app/assets/icons/win/icon.ico\" --prune=true --out=\"release/builds\" --version-string.CompanyName=\"sFTP Client\" --version-string.FileDescription=\"sFTP Client\" --version-string.ProductName=\"sFTP Client\"",
    "package-windows-app": "node ./scripts/windows/appx.js",
    "package-windows-app-legacy": "electron-windows-store --input-directory \"C:\\in\" --output-directory \"C:\\out\" --flatten true --package-version 3.3.16 --package-name \"sFTPClient\" --expanded-base-image \"C:\\ProgramData\\Microsoft\\Windows\\Images\\BaseImage_DAC_17134\" --windows-kit \"C:\\Program Files (x86)\\Windows Kits\\10\\App Certification Kit\" --publisher \"CN=sFTP Client Ltd, OU=IT, O=sFTP Client Ltd, STREET=Evergreen Turnpike Close Ardleigh, L=Colchester, S=Essex, PostalCode=CO7 7QW, C=GB\" --publisher-display-name \"sFTP Client Ltd\" --dev-cert \"C:\\out\\windows.pfx\" --cert-pass R00t3d2012!",
    "package-linux": "electron-packager . \"sftpclient\" --ignore=\"^/angular.json|^/tsconfig-serve.json|^/Jenkinsfile|^/.idea|^/.git|^/.editorconfig|^/.gitignore|^/karma.conf.js|^/package-lock.json|^/protractor.conf.js|^/README.md|^/tsconfig.json|^/tslint.json|^/e2e|^/src|^/scripts|^/snap|^/release|^/build\" --overwrite --platform=linux --arch=ia32 --icon=\"app/assets/icons/png/512x512.png\" --prune=true --out=\"release/builds\"",
    "package-linux64": "electron-packager . \"sftpclient\" --ignore=\"^/angular.json|^/tsconfig-serve.json|^/Jenkinsfile|^/.idea|^/.git|^/.editorconfig|^/.gitignore|^/karma.conf.js|^/package-lock.json|^/protractor.conf.js|^/README.md|^/tsconfig.json|^/tslint.json|^/e2e|^/src|^/scripts|^/snap|^/release|^/build\" --overwrite --platform=linux --arch=x64 --icon=\"app/assets/icons/png/512x512.png\" --prune=true --out=\"release/builds\"",
    "flatpak": "electron-installer-flatpak --src=\"release/builds/sFTP Client-linux-x86\" --dest=\"release/installers\" --arch x86",
    "flatpak64": "electron-installer-flatpak --src=\"release/builds/sFTP Client-linux-x64\" --dest=\"release/installers\" --arch x64",
    "zip-mac-installer": "node ./scripts/mac/archive.mac.js",
    "zip-mas-installer": "node ./scripts/mac/archive.mas.js",
    "zip-mas-lite-installer": "node ./scripts/mac/archive.mas.lite.js",
    "zip-windows-installer": "node ./scripts/windows/archive.js",
    "zip-linux-installer": "node ./scripts/linux/archive32.js && node ./scripts/linux/archive64.js",
    "create-linux-installer": "electron-installer-debian --options.productName \"sFTP Client\" --src \"./release/builds/sftpclient-linux-ia32/\" --dest release/installers/ --arch i386",
    "create-linux64-installer": "electron-installer-debian --options.productName \"sFTP Client\" --src \"./release/builds/sftpclient-linux-x64/\" --dest release/installers/ --arch amd64",
    "create-linux-snap": "electron-installer-snap --src=\"./release/builds/sFTP Client-linux-x64\"",
    "create-modern-windows-installer": "node ./scripts/windows/modern_installer.js",
    "create-windows-installer": "iscc /Qp ./scripts/windows/installer.iss",
    "sign-windows": "signtool sign /f \"./scripts/windows/cert.pfx\" /p R00t3d2012! /d \"sFTP Client\" /n \"sFTP Client Ltd\" /du \"https://www.sftpapp.com\" \"./release/builds/sFTP Client-win32-ia32/sFTP Client.exe\"",
    "sign-windows-installer": "signtool sign /f \"./scripts/windows/cert.pfx\" /p R00t3d2012! /d \"sFTP Client\" /n \"sFTP Client Ltd\" /du \"https://www.sftpapp.com\" \"./release/installers/Setup.exe\"",
    "snap": "build",
    "appx": "electron-windows-store --input-directory \"C:\\in\" --output-directory \"C:\\out\" --package-version 3.3.16 --dev-cert \"/\" --package-name \"sFTPClient\" --desktop-converter \"C:\\Program Files\\WindowsApps\\Microsoft.DesktopAppConverter_2.1.4.0_x64__8wekyb3d8bbwe\" --windows-kit \"C:\\Program Files (x86)\\Windows Kits\\10\bin\\10.0.17134.0\\x64\" --manifest \"C:\\out\\AppXManifest.xml\" --assets \"C:\\out\\assets\" --publisher \"CN=B2083CA0-6DAD-457B-AB20-AF87A0475EE5\" --identity-name \"sFTPClient.sFTPClient\" --publisher-display-name \"sFTP Client Ltd\"",
    "release-mac": "npm run package-mac && npm run zip-mac-installer",
    "release-linux": "npm run package-linux && npm run package-linux64 && npm run create-linux-installer && npm run create-linux64-installer && npm run zip-linux-installer",
    "release-windows": "npm run package-windows && npm run sign-windows && npm run create-windows-installer && npm run sign-windows-installer && npm run zip-windows-installer",
    "release": "npm run release-mac && npm run release-linux && npm run release-windows"
  },
  "build": {
    "appId": "io.snapcraft.sftpclient",
    "linux": {
      "icon": "app/assets/icons/png/512x512.png",
      "category": "Utility",
      "target": [
        "snap"
      ]
    },
    "win": {
      "target": [
        "appx"
      ]
    }
  },
  "icon": "app/assets/icons/512x512.png",
  "private": true,
